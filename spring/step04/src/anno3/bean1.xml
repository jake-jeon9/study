<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<context:component-scan base-package="anno3"/>

	<bean id="favoriteFood1" class="anno3.Food">
		<property name="foodName" value="짜파게티"/>
		<property name="foodPrice" value="7500"/>
	</bean>
	<bean id="unFavoriteFood1" class="anno3.Food">
		<property name="foodName" value="개보신탕"/>
		<property name="foodPrice" value="15000"/>
	</bean>
</beans>
